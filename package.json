{
  "name": "breakdance-cli",
  "description": "CLI for breakdance, the HTML to markdown converter for node.js.",
  "version": "1.0.1",
  "homepage": "https://github.com/breakdance/breakdance-cli",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "breakdance/breakdance-cli",
  "bugs": {
    "url": "https://github.com/breakdance/breakdance-cli/issues"
  },
  "license": "MIT",
  "files": [
    "cli.js"
  ],
  "bin": {
    "bd": "cli.js",
    "breakdance": "cli.js"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "breakdance": "^3.0.0",
    "log-ok": "^0.1.1",
    "minimist": "^1.2.0",
    "write": "^0.3.3"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.12"
  },
  "keywords": [
    "breakdance",
    "cli",
    "compile",
    "compiler",
    "convert",
    "html",
    "markdown",
    "parse",
    "parser",
    "render",
    "transform"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "breakdance",
      "breakdance-util",
      "snapdragon"
    ]
  }
}
